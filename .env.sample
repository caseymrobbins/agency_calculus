#.env.example
# This file provides a template for environment variables.
# Copy this to.env and fill in your actual values.
# DO NOT commit the.env file to version control.

# --- PostgreSQL Database Configuration ---
POSTGRES_DB=agency_monitor
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# --- FastAPI API Configuration ---
# Generate a secure secret key for production environments
# e.g., openssl rand -hex 32
SECRET_API_KEY=a_very_secret_key_for_dev

# --- Development Settings ---
DEBUG=true
LOG_LEVEL=INFO

# --- External API Keys (if needed) ---
# These are not used by the core application but are here for future expansion
WORLD_BANK_API_KEY=
VDEM_API_KEY=